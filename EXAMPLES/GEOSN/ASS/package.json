{
  "name": "ass",
  "version": "1.0.0",
  "description": "ASS API",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/server",
    "graphql-dev": "ts-node-dev --respawn src/graphql",
    "test": "ts-mocha tests/**/*.spec.ts",
    "test:watch": "ts-mocha tests/**/*.spec.ts -w --watch-files '**/*.ts'",
    "start": "node build/src/server"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "node-fetch-commonjs": "^3.3.2",
    "ts-mocha": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "promised-sqlite3": "^2.1.0",
    "sqlite3": "^5.1.6"
  }
}
